  ("j") ("k") ("l") ("m") ("n") ("o") ("p") ("q") ("r")
  ("s") ("t") ("u") ("v") ("w") ("x") ("y") ("z"))

 (consonant
  ("k" "କ୍")
  ("kh" "ଖ୍")
  ("g" "ଗ୍")
  ("gh" "ଘ୍")
  ("~N" "ଙ୍")
  ("N^" "ଙ୍")
  ("ch" "ଚ୍")
  ("Ch" "ଛ୍")
  ("chh" "ଛ୍")
  ("j" "ଜ୍")
  ("jh" "ଝ୍")
  ("~n" "ଞ୍")
  ("JN" "ଞ୍")
  ("T" "ଟ୍")
  ("Th" "ଠ୍")
  ("D" "ଡ୍")
  ("Dh" "ଢ୍")
  ("N" "ଣ୍")
  ("t" "ତ୍")
  ("th" "ଥ୍")
  ("d" "ଦ୍")
  ("dh" "ଧ୍")
  ("n" "ନ୍")
