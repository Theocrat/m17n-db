        <set id="y">
          <predefined-nth-previous-or-following-character position="-2"/>
        </set>
        <set id="z">
          <predefined-nth-previous-or-following-character position="-1"/>
        </set>
        <set id="arg1">
          <variable-reference id="y"/>
        </set>
        <set id="arg2">
          <variable-reference id="z"/>
        </set>
        <macro-reference id="macro-cp"/>
